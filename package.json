{
  "name": "gatsby-plugin-ackee-tracker",
  "version": "1.0.11",
  "description": "Gatsby plugin to add Ackee Tracker to a site.",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir .",
    "precommit": "lint-staged",
    "publish": "npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "ackee",
    "tracking",
    "tracker",
    "analytics",
    "statistics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/burnsy/gatsby-plugin-ackee-tracker.git"
  },
  "author": "Christopher Burns <christopher@tapkit.io>",
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "prettier": "1.16.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-env",
      "babel-preset-react"
    ]
  },
  "dependencies": {
    "ackee-tracker": "^3.2.1"
  }
}
